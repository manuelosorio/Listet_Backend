    "express",
    "backend",
    "fullstack"
  ],
  "author": "Manuel Osorio",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/manuelosorio/todo-list_backend/issues"
  },
  "homepage": "https://github.com/manuelosorio/todo-list_backend#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/mysql": "^2.15.10",
    "@types/passport": "^1.0.3",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
