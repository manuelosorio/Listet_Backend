{
  "name": "todo-list_backend",
  "version": "1.0.0",
  "description": "Backend api for ToDo List Web App",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src/ --exec \"tsc && npm start\" --verbose -e ts",
    "dev2": "concurrently -k -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"nodemon ./dist/app.js\"",
    "watch-ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelosorio/todo-list_backend.git"
  },
  "keywords": [
    "Todo",
    "list",
    "angular",
    "express",
    "backend",
    "fullstack"
  ],
  "author": "Manuel Osorio",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/manuelosorio/todo-list_backend/issues"
  },
  "homepage": "https://github.com/manuelosorio/todo-list_backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-mysql-session": "^2.1.4",
    "express-session": "^1.17.1",
    "helmet": "^3.23.1",
    "mysql": "^2.18.1",
    "snyk": "^1.401.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-flash": "0.0.2",
    "@types/express-mysql-session": "^2.1.1",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.47",
    "@types/mysql": "^2.15.10",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "snyk": true
}
