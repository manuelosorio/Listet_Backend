{
  "name": "listet_backend",
  "version": "1.0.0",
  "description": "Backend api for Listet",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'nodemon ./dist/server.js' -i true --default-input-target 1",
    "dev:verbose": "nodemon --watch src/ --exec 'tsc && npm start' --verbose -e ts",
    "watch-ts": "tsc -w",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "echo 'Error: no test specified' && exit 1",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test",
    "prepare": "npm run snyk:protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelosorio/Listet_Backend.git"
  },
  "keywords": [
    "Todo",
    "list",
    "angular",
    "express",
    "backend",
    "fullstack"
  ],
  "author": "Manuel Osorio",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/manuelosorio/Listet_Backend.git/issues"
  },
  "homepage": "https://github.com/manuelosorio/Listet_Backend.git#readme",
  "dependencies": {
    "@socket.io/cluster-adapter": "^0.5.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "concurrently": "^6.5.1",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "email-templates": "^8.1.0",
    "express": "^4.19.2",
    "express-flash": "0.0.2",
    "express-mysql-session": "^2.1.8",
    "express-session": "^1.17.3",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.1",
    "pug": "^3.0.2",
    "@snyk/protect": "^1.1104.0",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/email-templates": "^8.0.4",
    "@types/express": "^4.17.17",
    "@types/express-flash": "0.0.2",
    "@types/express-mysql-session": "^2.1.3",
    "@types/express-session": "^1.17.6",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^14.18.36",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^3.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "typescript": "^4.9.5"
  },
  "snyk": true,
  "private": true
}
